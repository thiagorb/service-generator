{
    "name": "thiagorb/service-generator",
    "require": {
        "php": "^7.1",
        "nikic/php-parser": "^4.1",
        "phpdocumentor/reflection-docblock": "^4.3 || ^5",
        "nette/php-generator": "^3.2",
        "thiagorb/service-generator-runtime": "^0.2",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "vimeo/psalm": "*"
    },
    "authors": [
        {
            "name": "Thiago Barcala",
            "email": "thiago.barcala@gmail.com"
        }
    ],
    "bin": ["bin/service-generator"],
    "autoload": {
        "psr-4": {
            "Thiagorb\\ServiceGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
